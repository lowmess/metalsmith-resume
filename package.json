{
  "name": "metalsmith-resume",
  "version": "0.0.6",
  "description": "A resume generator, built with Metalsmith",
  "main": "build.js",
  "engines": {
    "node": "5.9.0"
  },
  "scripts": {
    "start": "npm run browser_sync & NODE_ENV=development nodemon --exec 'npm run build && browser-sync reload'",
    "browser_sync": "browser-sync start --server _build --no-ui --no-notify",
    "build": "node build.js",
    "build:minify": "NODE_ENV=production npm run build",
    "pdf": "NODE_ENV=print npm run build && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lowmess/metalsmith-resume.git"
  },
  "keywords": [
    "metalsmith",
    "resume",
    "cv",
    "static",
    "site",
    "skeleton",
    "theme"
  ],
  "author": "Alec Lomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lowmess/metalsmith-resume/issues"
  },
  "homepage": "https://github.com/lowmess/metalsmith-resume#readme",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "colors.css": "^3.0.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.7.1",
    "get-contrast": "^2.0.0",
    "html-pdf": "^2.0.1",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-html-minifier": "^2.0.0",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdownit": "^0.3.0",
    "metalsmith-metadata": "0.0.4",
    "metalsmith-permalinks": "^0.5.0",
    "moment": "^2.13.0",
    "normalize.css": "^4.1.1",
    "postcss": "^5.0.21",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-focus": "^1.0.0",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "postcss-uncss": "0.0.1",
    "pug": "^2.0.0-alpha8",
    "tachyons": "^4.0.4",
    "tachyons-columns": "^1.0.0",
    "tachyons-print": "^4.0.4",
    "uncss": "^0.13.0"
  },
  "devDependencies": {
    "browser-sync": "^2.12.10",
    "cz-conventional-changelog": "^1.2.0",
    "nodemon": "^1.9.2",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
